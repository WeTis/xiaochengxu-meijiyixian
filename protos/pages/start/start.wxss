.index{
  position: fixed;
  top: 0;
  bottom: 0;
  width: 100%;
}
image{
  width: 100%;
  height: 100%;
}
.tiyan{
  display: inline-block;
  width: 220rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  margin-left: -110rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 4rpx solid #5a7009;
  letter-spacing: 10rpx;
  font-size: 30rpx;
  color: #fff;
  border-radius: 10rpx;
  bottom: 100rpx;
}
.tiyan button{
  height: 60rpx;
  line-height: 60rpx;
  border: none;
  background-color:rgba(0, 0, 0, 0);
  font-size: 30rpx;
  color: #fff;
}

